//This file is only used when deployed on Azure Static Web Apps.

{
  "trailingSlash": "auto",
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/img/*.{png,jpg,gif}", "/css/*"]
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'none' ; object-src 'none' ; img-src *"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
